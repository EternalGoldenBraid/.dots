# UltiSnips for LaTeX in Markdown
snippet mk "Inline" A
$$1$
endsnippet

snippet dm "Math mode" A
$$
$1
$$
endsnippet

snippet beg "Description" w
\\begin{$1}
$0
\\end{$1}
endsnippet

# Greek letters

snippet @a "Greek letter" A
\alpha
endsnippet

snippet @A "Greek letter" mA
\Alpha
endsnippet

snippet @b "Greek letter" mA
\beta
endsnippet

snippet @B "Greek letter" mA
\Beta
endsnippet

snippet @c "Greek letter" mA
\chi
endsnippet

snippet @C "Greek letter" mA
\Chi
endsnippet

snippet @U "Greek letter" mA
\Upsilon
endsnippet

snippet ome "Greek letter" mA
\omega
endsnippet

snippet @o "Greek letter" mA
\omega
endsnippet

snippet @O "Greek letter" mA
\Omega
endsnippet

snippet @u "Greek letter" mA
\upsilon
endsnippet

snippet @U "Greek letter" mA
\Upsilon
endsnippet


# Operations
#{trigger: "te", replacement: "\\text{$0}", options: "m"},
snippet te "Text" 
\text{$1}
endsnippet

#{trigger: "bf", replacement: "\\mathbf{$0}", options: "mA"},
snippet bf "Bold"
\mathbf{$1}
endsnippet

#{trigger: "sr", replacement: "^{2}", options: "mA"},
snippet sr "Square"
^{2}
endsnippet

#{trigger: "cb", replacement: "^{3}", options: "mA"},
snippet cb "Cube"
^{3}
endsnippet

#{trigger: "rd", replacement: "^{$0}$1", options: "mA"},
snippet rd "Power" i
^{$1}$2
endsnippet

#{trigger: "_", replacement: "_{$0}$1", options: "mA"},
snippet _ "Subscript" i
_{$1}$2
endsnippet

#{trigger: "sts", replacement: "_\\text{$0}", options: "rmA"},
snippet sts "Subscript text"
_{\text{$1}}
endsnippet

#{trigger: "sq", replacement: "\\sqrt{ $0 }$1", options: "mA"},
snippet sq "Square root"
\sqrt{ $1 }$2
endsnippet

#{trigger: "//", replacement: "\\frac{$0}{$1}$2", options: "mA"},
snippet // "Fraction" iA
\frac{ $1 }{ $2 }$3
endsnippet

#{trigger: "ee", replacement: "e^{ $0 }$1", options: "mA"},
snippet ee "Exponential"
e^{ $1 }$2
endsnippet

#{trigger: "rm", replacement: "\\mathrm{$0}$1", options: "mA"},
snippet rm "Roman"
\mathrm{$1}
endsnippet

#{trigger: "conj", replacement: "^{*}", options: "mA"},
#{trigger: "trace", replacement: "\\mathrm{Tr}", options: "mA"},
#{trigger: "det", replacement: "\\det", options: "mA"},
snippet det "Determinant"
\det
endsnippet

#{trigger: "re", replacement: "\\mathrm{Re}", options: "mb"},
#{trigger: "im", replacement: "\\mathrm{Im}", options: "mb"},


snippet bar "Math operator" mA
\bar{$1}
endsnippet

